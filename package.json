    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.spec.+(ts|tsx|js)"
    ]
  },
  "author": "Masatomi KINO <masatomix@ki-no.org> (https://qiita.com/masatomix)",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@types/unzip": "^0.1.1",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "config": "^3.2.5",
    "csvtojson": "^2.0.10",
